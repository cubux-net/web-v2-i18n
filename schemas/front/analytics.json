{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$defs": {
    "string": {
      "type": "string",
      "minLength": 1
    }
  },
  "type": "object",
  "properties": {
    "filter": {
      "type": "object",
      "properties": {
        "button": {
          "type": "object",
          "properties": {
            "ExportExcel": {
              "$ref": "#/$defs/string"
            }
          },
          "required": [
            "ExportExcel"
          ]
        },
        "chart": {
          "type": "object",
          "properties": {
            "Chart": {
              "$ref": "#/$defs/string"
            }
          },
          "required": [
            "Chart"
          ]
        },
        "date": {
          "type": "object",
          "properties": {
            "Period": {
              "$ref": "#/$defs/string"
            },
            "SetPeriod": {
              "$ref": "#/$defs/string"
            },
            "SetYear": {
              "$ref": "#/$defs/string"
            }
          },
          "required": [
            "Period",
            "SetPeriod",
            "SetYear"
          ]
        },
        "groupBy": {
          "type": "object",
          "properties": {
            "Categories": {
              "$ref": "#/$defs/string"
            },
            "Months": {
              "$ref": "#/$defs/string"
            }
          },
          "required": [
            "Categories",
            "Months"
          ]
        },
        "limit": {
          "type": "object",
          "properties": {
            "Accounts": {
              "$ref": "#/$defs/string"
            },
            "Categories": {
              "$ref": "#/$defs/string"
            },
            "ExtraFilters": {
              "$ref": "#/$defs/string"
            },
            "Projects": {
              "$ref": "#/$defs/string"
            },
            "Users": {
              "$ref": "#/$defs/string"
            }
          },
          "required": [
            "Accounts",
            "Categories",
            "ExtraFilters",
            "Projects",
            "Users"
          ]
        },
        "type": {
          "type": "object",
          "properties": {
            "Expenses": {
              "$ref": "#/$defs/string"
            },
            "Incomes": {
              "$ref": "#/$defs/string"
            },
            "WithCredits": {
              "$ref": "#/$defs/string"
            },
            "WithLoans": {
              "$ref": "#/$defs/string"
            },
            "WithTransfers": {
              "$ref": "#/$defs/string"
            }
          },
          "required": [
            "Expenses",
            "Incomes",
            "WithCredits",
            "WithLoans",
            "WithTransfers"
          ]
        }
      },
      "required": [
        "button",
        "chart",
        "date",
        "groupBy",
        "limit",
        "type"
      ]
    },
    "page": {
      "type": "object",
      "properties": {
        "Title": {
          "$ref": "#/$defs/string"
        },
        "message": {
          "type": "object",
          "properties": {
            "NoChartData": {
              "$ref": "#/$defs/string"
            }
          },
          "required": [
            "NoChartData"
          ]
        }
      },
      "required": [
        "Title",
        "message"
      ]
    }
  },
  "required": [
    "filter",
    "page"
  ]
}
